{
  "name": "eslint-plugin-galaxy",
  "version": "1.19.0",
  "description": "Various ESLint rules in one plugin.",
  "files": [
    "dist",
    "docs"
  ],
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --fix .",
    "prepack": "rm -rf dist && tsc",
    "release": "release-it",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/eslint-plugin-galaxy.git"
  },
  "bugs": {
    "url": "https://github.com/CyanSalt/eslint-plugin-galaxy/issues"
  },
  "homepage": "https://github.com/CyanSalt/eslint-plugin-galaxy#readme",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/utils": "^5.10.2",
    "globals": "^13.20.0"
  },
  "devDependencies": {
    "@cyansalt/eslint-config-preset": "^1.4.3",
    "@cyansalt/tsconfig": "^2.1.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.22",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.4.1",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "jest": "^27.5.0",
    "release-it": "^15.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "eslint-plugin-import": ">=2.25.4",
    "eslint-plugin-vue": ">=7.19.1",
    "eslint-plugin-vue-scoped-css": ">=2.2.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-plugin-vue-scoped-css": {
      "optional": true
    }
  }
