{
  "name": "eslint-plugin-galaxy",
  "version": "1.35.0",
  "description": "Various ESLint rules in one plugin.",
  "files": [
    "dist",
    "docs"
  ],
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint --fix './**/*.{js,mjs,ts}'",
    "prepack": "rm -rf dist && tsc",
    "release": "release-it",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/eslint-plugin-galaxy.git"
  },
  "bugs": {
    "url": "https://github.com/CyanSalt/eslint-plugin-galaxy/issues"
  },
  "homepage": "https://github.com/CyanSalt/eslint-plugin-galaxy#readme",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/utils": "^8.1.0",
    "browserslist": "^4.23.3",
    "caniuse-lite": "^1.0.30001651",
    "esquery": "^1.6.0",
    "globals": "^13.24.0",
    "ignore": "^5.3.2",
    "micromatch": "^4.0.8"
  },
  "devDependencies": {
    "@cyansalt/eslint-config": "^4.1.0",
    "@cyansalt/tsconfig": "^2.2.1",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/esquery": "^1.5.4",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^18.19.44",
    "@typescript-eslint/parser": "^8.1.0",
    "@typescript-eslint/rule-tester": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-vue-scoped-css": "^2.8.1",
    "release-it": "^17.6.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "vue-eslint-parser": "^9.4.3"
  },
  "peerDependencies": {
    "eslint": ">=8.44.0",
    "eslint-plugin-import-x": ">=0.5.1 <=4.9.4",
    "eslint-plugin-vue": ">=9.19.0",
    "eslint-plugin-vue-scoped-css": ">=2.6.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-import-x": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-plugin-vue-scoped-css": {
      "optional": true
    }
  }
}
