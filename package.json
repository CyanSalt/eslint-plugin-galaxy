{
  "name": "eslint-plugin-galaxy",
  "version": "1.36.0",
  "description": "Various ESLint rules in one plugin.",
  "files": [
    "dist",
    "docs"
  ],
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint --fix './**/*.{js,mjs,ts}'",
    "prepack": "rm -rf dist && tsc",
    "release": "release-it",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/eslint-plugin-galaxy.git"
  },
  "bugs": {
    "url": "https://github.com/CyanSalt/eslint-plugin-galaxy/issues"
  },
  "homepage": "https://github.com/CyanSalt/eslint-plugin-galaxy#readme",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/utils": "^8.35.1",
    "browserslist": "^4.25.1",
    "caniuse-lite": "^1.0.30001726",
    "esquery": "^1.6.0",
    "globals": "^16.3.0",
    "ignore": "^7.0.5",
    "micromatch": "^4.0.8"
  },
  "devDependencies": {
    "@cyansalt/eslint-config": "^4.3.1",
    "@cyansalt/tsconfig": "^2.2.1",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/esquery": "^1.5.4",
    "@types/micromatch": "^4.0.9",
