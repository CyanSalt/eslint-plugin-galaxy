{
  "name": "eslint-plugin-galaxy",
  "version": "1.11.0",
  "description": "Various ESLint rules in one plugin.",
  "files": [
    "dist",
    "docs"
  ],
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --fix .",
    "prepack": "rm -rf dist && tsc",
    "release": "release-it",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/eslint-plugin-galaxy.git"
  },
  "bugs": {
    "url": "https://github.com/CyanSalt/eslint-plugin-galaxy/issues"
  },
  "homepage": "https://github.com/CyanSalt/eslint-plugin-galaxy#readme",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.0.0"
  },
  "devDependencies": {
    "@cyansalt/eslint-config-preset": "^1.0.2",
    "@cyansalt/tsconfig": "^1.2.0",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.5",
    "release-it": "^14.11.8",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "eslint-plugin-import": ">=2.25.4",
    "eslint-plugin-vue": ">=7.19.1"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    }
  }
