    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/eslint-plugin-galaxy.git"
  },
  "bugs": {
    "url": "https://github.com/CyanSalt/eslint-plugin-galaxy/issues"
  },
  "homepage": "https://github.com/CyanSalt/eslint-plugin-galaxy#readme",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/utils": "^6.4.1",
    "globals": "^13.20.0",
    "ignore": "^5.2.4"
  },
  "devDependencies": {
    "@cyansalt/eslint-config-preset": "^1.15.0",
    "@cyansalt/tsconfig": "^2.2.1",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
